{
  "name": "template-cdk-lambda-base",
  "version": "0.1.0",
  "bin": {
    "template-cdk-lambda-base": "bin/template-cdk-lambda-base.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --coverage",
    "cdk": "cdk",
    "deploy": "cdk  deploy -v --profile user1",
    "cdk:bootstrap": "cdk bootstrap --profile user1",
    "reinstall": "rm -rf node-modules/ package-lock.json && npm install",
    "format": "npm run fix:eslint && npm run fix:prettier",
    "fix:prettier": "prettier --loglevel silent --config prettier.config.js --write '**/*.ts'",
    "fix:eslint": "eslint --fix '**/*.ts'",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:prettier": "prettier --config prettier.config.js --check '**/*.ts'"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/aws-lambda": "^8.10.97",
    "@types/aws-sdk": "^2.7.0",
    "@types/crypto-js": "^4.1.1",
    "@types/git-branch": "^2.0.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.11.9",
    "@types/prettier": "2.6.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "aws-cdk": "^2.59.0",
    "aws-sdk-js-codemod": "^0.14.1",
    "esbuild": "^0.16.17",
    "esbuild-node-tsc": "^2.0.5",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@aws-cdk/aws-appsync": "^1.182.0",
    "@aws-cdk/aws-appsync-alpha": "^2.54.0-alpha.0",
    "@aws-sdk/client-dynamodb": "^3.236.0",
    "@aws-sdk/client-s3": "^3.238.0",
    "@aws-sdk/client-ssm": "^3.296.0",
    "@aws-sdk/util-dynamodb": "^3.236.0",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/serverless": "^7.38.0",
    "@sirge-io/sirge-types": "^1.0.65",
    "@sirge-io/sirge-utils": "^1.0.22",
    "amazon-cognito-identity-js": "^6.1.2",
    "aws-cdk-lib": "^2.54.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.0.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "git-branch": "^2.0.1",
    "mongodb": "^5.1.0",
    "serverless-postgres": "^2.0.0",
    "source-map-support": "^0.5.21",
    "stripe": "^11.11.0"
  },
  "moduleDirectories": [
    "node_modules"
  ]
}
